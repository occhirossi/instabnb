{% extends 'base.html.twig' %}
    {% block body %}
            <div style="text-align: center" class="row">
        {% for tableau in tableau %}
                <div class="col s4 m4">
                    <div class="card blue-grey darken-1">
                        <div class="card-content white-text">

                            <p> {{ tableau.id }}<br/>
                                {{ tableau.title }}<br>
                                {{ tableau.content }}<br/>
                                 {{ tableau.price }}<br/>
                              {{ tableau.createdAt | date("m/d/Y") }}</p><br/>
                            <a href="{{ path('annoncementsDetail',{'id':tableau.id}) }}" class="waves-effect waves-light btn">detail</a>
                        </div>
                    </div>
                </div>
        {% endfor %}
            </div>

    {% endblock body %}